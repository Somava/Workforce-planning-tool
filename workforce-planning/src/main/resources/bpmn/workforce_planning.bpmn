    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="Flow_TriggerToWait" sourceRef="Activity_TriggerExternalSourcing" targetRef="Message_ExternalResponseReceived" />
    <bpmn:sequenceFlow id="Flow_ResponseToGateway" sourceRef="Message_ExternalResponseReceived" targetRef="Gateway_ExternalResourceOutcome" />
    <bpmn:sequenceFlow id="Flow_ExternalSuccess" name="Found" sourceRef="Gateway_ExternalResourceOutcome" targetRef="Activity_AssignEmployeePlanner">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">externalResourceFound == true</bpmn:conditionExpression>
